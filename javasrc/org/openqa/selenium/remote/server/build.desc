
# note that this server doesn't include the necessary drivers.

java_library(name = "server",
  srcs = [
    "DefaultDriverFactory.java",
    "DefaultDriverSessions.java",
    "DriverFactory.java",
    "DriverServlet.java",
    "DriverSessions.java",
    "KnownElements.java",
    "JsonParametersAware.java",
    "Session.java",
    "SessionCleaner.java",
    "SnapshotScreenListener.java",
    "handler/**/*.java",
    "renderer/*.java",
    "rest/*.java",
  ],
  deps = [
    "//javasrc/org/openqa/selenium:webdriver-api",
    "//java/client/src/org/openqa/selenium/remote",
    "//javasrc/org/openqa/selenium/support/events",
    "//third_party/java/guava-libraries",
    "//third_party/java/servlet-api",
  ])
