java_library(name = "resource",
  srcs = [
    "StaticResource.java",
  ],
  deps = [
    "//java/server/src/org/openqa/selenium/remote/server:restish",
  ],
  embedded = [
    ":client",
    ":hub",
    ":style",
  ])

rename(name = "client",
  deps = ["//javascript/remote:clientbin.js"],
  out = "client.js")

rename(name = "hub",
  deps = ["//javascript/remote:hub.html"],
  out = "hub.html")

rename(name = "style",
  deps = ["//javascript/remote:style.css"],
  out = "style.css")
